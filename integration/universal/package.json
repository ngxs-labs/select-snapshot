{
  "name": "universal-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist-server/server",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:ssr": "node dist-server/server",
    "build:server": "webpack --progress --colors",
    "build:ssr": "yarn build:ssr:bundle && yarn build:server",
    "build:ssr:bundle": "ng build --prod && ng run universal-app:server:production",
    "test:ssr": "yarn build:ssr && concurrently \"yarn start:ssr\" \"yarn ng test --runInBand\"",
    "postinstall": "cpx -v -C \"../../dist/select-snapshot/**/*\" \"node_modules/@ngxs-labs/select-snapshot/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.1",
    "@angular/common": "file:../../node_modules/@angular/common",
    "@angular/compiler": "file:../../node_modules/@angular/compiler",
    "@angular/core": "file:../../node_modules/@angular/core",
    "@angular/platform-browser": "file:../../node_modules/@angular/platform-browser",
    "@angular/platform-browser-dynamic": "file:../../node_modules/@angular/platform-browser-dynamic",
    "@angular/platform-server": "8.0.1",
    "@nguniversal/express-engine": "7.1.1",
    "@ngxs/store": "file:../../node_modules/@ngxs/store",
    "express": "4.17.1",
    "reflect-metadata": "file:../../node_modules/reflect-metadata",
    "rxjs": "file:../../node_modules/rxjs",
    "zone.js": "file:../../node_modules/zone.js"
  },
  "devDependencies": {
    "@angular-builders/jest": "file:../../node_modules/@angular-builders/jest",
    "@angular-devkit/build-angular": "file:../../node_modules/@angular-devkit/build-angular",
    "@angular/cli": "file:../../node_modules/@angular/cli",
    "@angular/compiler-cli": "file:../../node_modules/@angular/compiler-cli",
    "@types/express": "4.17.0",
    "@types/jest": "file:../../node_modules/@types/jest",
    "@types/node": "file:../../node_modules/@types/node",
    "@types/webpack": "4.4.32",
    "concurrently": "4.1.0",
    "cpx": "1.5.0",
    "jest": "file:../../node_modules/jest",
    "ts-loader": "6.0.2",
    "ts-node": "file:../../node_modules/ts-node",
    "tslib": "file:../../node_modules/tslib",
    "typescript": "file:../../node_modules/typescript",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4"
  }
}
