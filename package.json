{
  "name": "select-snapshot",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxs-labs/select-snapshot.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngxs-labs/select-snapshot#readme",
  "bugs": {
    "url": "https://github.com/ngxs-labs/select-snapshot/issues"
  },
  "keywords": [
    "ngxs",
    "redux",
    "store"
  ],
  "scripts": {
    "ng": "ng",
    "lint": "ng lint select-snapshot",
    "test": "ng test --run-in-band",
    "build": "ng-packagr -p src/package.json && cpx README.md dist",
    "format": "prettier src/**/*.ts --write",
    "integration:install": "yarn --cwd integration/universal install",
    "integration:build": "yarn --cwd integration/universal build:ssr",
    "integration:test": "yarn --cwd integration/universal test:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^11.2.11",
    "@angular/compiler": "^11.2.11",
    "@angular/core": "^11.2.11",
    "@angular/platform-browser": "^11.2.11",
    "@angular/platform-browser-dynamic": "^11.2.11",
    "@ngxs/store": "^3.7.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.2.0",
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular/cli": "^11.2.10",
    "@angular/compiler-cli": "^11.2.11",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.41",
    "codelyzer": "^6.0.1",
    "cpx": "^1.5.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^11.2.4",
    "npm-run-all": "4.1.5",
    "prettier": "^2.2.1",
    "ts-node": "8.10.2",
    "tsickle": "~0.39.1",
    "tslint": "~6.1.3",
    "typescript": "4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,html,scss,ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": true,
    "endOfLine": "lf",
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "singleQuote": true
  }
}
