{
  "name": "select-snapshot",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxs-labs/select-snapshot.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngxs-labs/select-snapshot#readme",
  "bugs": {
    "url": "https://github.com/ngxs-labs/select-snapshot/issues"
  },
  "keywords": [
    "ngxs",
    "redux",
    "store"
  ],
  "scripts": {
    "ng": "ng",
    "lint": "ng lint select-snapshot",
    "test": "ng test --runInBand",
    "build": "ng-packagr -p src/package.json && cpx README.md dist",
    "format": "prettier src/**/*.ts --write",
    "integration:install": "yarn --cwd integration/universal install",
    "integration:build": "yarn --cwd integration/universal build:ssr",
    "integration:test": "yarn --cwd integration/universal test:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@ngxs/store": "^3.6.2",
    "rxjs": "^6.5.5",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jest": "^25.2.3",
    "@types/node": "12.0.8",
    "codelyzer": "^5.2.2",
    "cpx": "^1.5.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.6",
    "ng-packagr": "^9.1.5",
    "npm-run-all": "4.1.5",
    "prettier": "^2.0.5",
    "ts-node": "8.10.2",
    "tsickle": "~0.38.1",
    "tslib": "^1.12.0",
    "tslint": "^6.1.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,html,scss,ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": true,
    "endOfLine": "lf",
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "singleQuote": true
  }
}
